{
    "manifest_version": 2,

    "name": "safe-search",
    "description": "Safe Search on Google, YouTube, Bing, Yahoo, DuckDuckGo, Ixquick, and Startpage",
    "version": "0.2.0",

    "icons": {
        "128": "icon.png"
    },

    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "<all_urls>",
        "cookies",
        "webRequest",
        "webRequestBlocking"
    ]
}
